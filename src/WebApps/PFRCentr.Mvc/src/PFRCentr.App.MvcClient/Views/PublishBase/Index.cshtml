@model PFRCentr.App.MvcClient.Controllers.CatalogIndexViewModel

@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<div class="container p-4 border">
    <div class="row pb-2">
        <div class="col">
            <h1 class="text-primary">Database</h1>
        </div>
    </div>
    @Html.Partial("_Pagination", Model.PaginationInfo)
    <div class="col-md" style="overflow-x: auto">
        <table class="table table-bordered" style="width:auto">
            <thead>
            <tr>
                <th>
                    image
                </th>
                <th>
                    Id
                </th>
                <th>
                    CategoryId
                </th>
                <th>
                    CreatorId
                </th>
                <th>
                    CustomerId
                </th>
                <th>
                    ExpiredDate
                </th>
                <th>
                    IsVisible
                </th>
                <th>
                    PublishCode
                </th>
                <th>
                    PublishDate
                </th>
                <th>
                    OwnerAccountId
                </th>
            </tr>
            </thead>

            <tbody>

            @foreach (var item in Model.CatalogItems)
            {
                <tr>
                    <td>
                        <img width="100" height="100" src="data:image/png;base64," class="detailImage"/>
                    </td>
                    <td>@item.Id</td>
                    <td>@item.CategoryId</td>
                    <td>@item.CreatorId</td>
                    <td>@item.CustomerId</td>
                    <td>@item.ExpiredDate</td>
                    <td>@item.IsVisible</td>
                    <td>@item.PublishCode</td>
                    <td>@item.PublishDate</td>
                    <td>@item.OwnerAccountId</td>
                </tr>
            }

            </tbody>
        </table>
        @Html.Partial("_Pagination", Model.PaginationInfo)
    </div>
</div>
